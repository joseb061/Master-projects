library(combinat)
x= c(39,29,60,40,32)
y=c(11,5,20,8,6)
sttrue= cor(x,y)
n=length(y)
nr=fact(n) #number of rearrangements to be examined
st=numeric(nr)
cnt=0
d=permn(y)
for (i in 1:nr){
st[i]<-cor(d[[i]],x)
if (st[i] >=sttrue)
cnt=cnt+1
}
print(paste("p-value= ",cnt/nr))
hist(st)
abline(v=sttrue,col="blue",lwd=2)
