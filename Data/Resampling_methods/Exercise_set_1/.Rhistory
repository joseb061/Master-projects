df<-data.frame(TEMP = c(75,83,85,85,92,97,99),
WC = c(16,20,25,27,32,48,48),
TMG=c(1.85,1.25,1.5,1.75,1.15,1.75,1.6))
model <- lm(WC ~ TEMP+TMG,data=df)
summary(model)
